    inner join enumeration enum2 on enum2.enum_id=ds_training_schedule.training_course
    inner join ds_signup_student on ds_signup_student.id=ds_student_reservation.student_id
    where ds_training_schedule.training_date=#{date,jdbcType=DATE}
    and ds_training_schedule.training_city=#{cityId,jdbcType=VARCHAR}
    order by trainingTime,coachName
